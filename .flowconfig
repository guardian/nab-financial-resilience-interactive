[ignore]
<PROJECT_ROOT>/public
<PROJECT_ROOT>/docs
<PROJECT_ROOT>/build
<PROJECT_ROOT>/node_modules/fbjs/.*
<PROJECT_ROOT>/node_modules/promise/.*
<PROJECT_ROOT>/node_modules/stylelint/*
<PROJECT_ROOT>/node_modules/http-parser-js/*
<PROJECT_ROOT>/node_modules/styled-components/src/.*
<PROJECT_ROOT>/node_modules/radium/src/.*
<PROJECT_ROOT>/node_modules/polished/*

[include]

[libs]
<PROJECT_ROOT>/node_modules/styled-components/flow-typed/*.js
<PROJECT_ROOT>/node_modules/styled-components
<PROJECT_ROOT>/node_modules/polished
<PROJECT_ROOT>/node_modules/promise

[options]
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
module.system.node.resolve_dirname=node_modules
experimental.strict_type_args=true
munge_underscores=true
suppress_type=$FlowIssue
suppress_type=$FlowFixMe
suppress_type=$FixMe
