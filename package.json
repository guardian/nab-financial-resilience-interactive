{
  "name": "glabs-nab-financial-resilience-2017-quiz",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://interactive.guim.co.uk",
  "scripts": {
    "start": "node_modules/.bin/gulp clean && node scripts/start.js",
    "build": "node scripts/build.js",
    "staging": "node_modules/.bin/gulp clean && NODE_ENV='staging' node scripts/build.js && gulp cname && surge build",
    "test": "node scripts/test.js --env=jsdom",
    "deploy": "node_modules/.bin/gulp clean && node scripts/createEnv.js && node scripts/build.js && gulp deploy",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "polished": "^1.7.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-proptypes": "^1.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "redux": "^3.7.2",
    "styled-components": "^2.2.0",
    "victory": "^0.24.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.12",
    "@storybook/addon-links": "^3.2.12",
    "@storybook/react": "^3.2.12",
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-styled-components": "^1.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-react-app": "^3.0.3",
    "babel-preset-stage-1": "^6.24.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "^2.1.0",
    "css-loader": "^0.28.7",
    "del": "^3.0.0",
    "dotenv": "^4.0.0",
    "enzyme": "^3.1.0",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.4",
    "flow-bin": "^0.56.0",
    "fs-extra": "^4.0.2",
    "gulp": "^3.9.1",
    "gulp-dotenv": "^0.2.1",
    "gulp-file": "^0.3.0",
    "gulp-s3-upload": "^1.6.6",
    "gulp-util": "^3.0.8",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "^2.0.6",
    "prettier-eslint": "^8.2.0",
    "promise": "^8.0.1",
    "react-dev-utils": "^4.1.0",
    "react-hot-loader": "next",
    "react-test-renderer": "^16.0.0",
    "request-promise-native": "^1.0.5",
    "run-sequence": "^2.2.0",
    "style-loader": "^0.19.0",
    "surge": "^0.19.0",
    "sw-precache-webpack-plugin": "^0.11.4",
    "url-loader": "^0.5.9",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-manifest-plugin": "^1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": "last 2 versions"
          },
          "loose": true,
          "modules": false
        }
      ],
      "stage-1",
      "react"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-runtime"
    ],
    "env": {
      "development": {
        "plugins": [
          [
            "styled-components",
            {
              "ssr": false,
              "displayName": true,
              "preprocess": false
            }
          ],
          "react-hot-loader/babel"
        ]
      },
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR",
    "iOS 8"
  ]
}
